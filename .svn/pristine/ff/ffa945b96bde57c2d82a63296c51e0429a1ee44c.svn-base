				,layedit = layui.layedit
				,laydate = layui.laydate
				,$=layui.jquery;
		//创建一个编辑器
		var editIndex = layedit.build('LAY_demo_editor');

		//自定义验证规则
		form.verify({
			dlh: function(value){
				if(value.length < 5){
					return '标题至少得5个字符啊';
				}
			}
			,pass: [/^[\S]{6,12}$/,'密码必须6到12位，且不能出现空格' ]
			,content: function(value){
				layedit.sync(editIndex);
			}
		});
		//监听提交
		form.on('submit(demo1)', function(data){
			//window.open("${pageContext.request.contextPath}/gys/gyszc.action");
			//window.close();
		});
		form.on('radio(yy_buttonChoseYes)', function(data){
		     	//$('.layui-btn').css('background','#1aa094');
				//$('.layui-btn').attr('disabled',false);
				$('.yy_mytzA').css('background','#1aa094');
				$('.yy_mytzA').attr('disabled',false);
				$('.yy_mytzA').attr('href','${pageContext.request.contextPath}/gys/gyszc.action');
			});
		form.on('radio(yy_buttonChoseNo)', function(data){
			//$('.layui-btn').css('background','#ccc');
			//$('.layui-btn').attr('disabled',true);
			$('.yy_mytzA').removeAttr('href');
			$('.yy_mytzA').css('background','#ccc');
			$('.yy_mytzA').setAttribute('disabled',true);
		});

	});

